{
  "name": "backend-voxecho-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["@vox-echo-workspace/backend"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2e}"],
      "options": {
        "jestConfig": "apps/backend-voxecho-e2e/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/backend-voxecho-e2e/**/*.{js,ts}"]
      }
    }
  }
}
